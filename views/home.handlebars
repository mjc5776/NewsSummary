
<div class = "container">
{{#each articles }}
<div class="card">
  <div class="card-header">
    Article
  </div>
  <div class="card-body">
    <h5 class="card-title">{{title}}</h5>
    <p class="card-text">{{link}}</p>
    <button type="button" class="btn btn-success savearticle" id = {{id}}>Save Article</button>
  </div>
</div>
{{else}}
<p>No articles to display</p>
{{/each}}

</div>